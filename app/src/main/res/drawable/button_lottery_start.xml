<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="22dip" />
            <stroke android:width="1dip" android:color="@color/btnMainPressedBorderColorLotteryStart" />
            <gradient android:angle="-90" android:startColor="@color/btnMainPressedStartColorLotteryStart" android:endColor="@color/btnMainPressedEndColorLotteryStart"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="22dip" />
            <stroke android:width="1dip" android:color="@color/btnMainPressedBorderColorLotteryStart" />
            <solid android:color="@color/btnMainShapeEndColorLotteryStart"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="22dip" />
            <stroke android:width="1dip" android:color="@color/btnMainShapeBorderColorLotteryStart" />
            <gradient android:angle="-90" android:startColor="@color/btnMainShapeStartColorLotteryStart" android:endColor="@color/btnMainShapeEndColorLotteryStart" />
        </shape>
    </item>
</selector>