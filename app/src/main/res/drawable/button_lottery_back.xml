<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="22dip" />
            <stroke android:width="1dip" android:color="@color/btnMainPressedBorderColorLotteryBack" />
            <gradient android:angle="-90" android:startColor="@color/btnMainPressedStartColorLotteryBack" android:endColor="@color/btnMainPressedEndColorLotteryBack"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="22dip" />
            <stroke android:width="1dip" android:color="@color/btnMainPressedBorderColorLotteryBack" />
            <solid android:color="@color/btnMainShapeEndColorLotteryBack"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="22dip" />
            <stroke android:width="1dip" android:color="@color/btnMainShapeBorderColorLotteryBack" />
            <gradient android:angle="-90" android:startColor="@color/btnMainShapeStartColorLotteryBack" android:endColor="@color/btnMainShapeEndColorLotteryBack" />
        </shape>
    </item>
</selector>