<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="22dip" />
            <stroke android:width="1dip" android:color="@color/btnMainPressedBorderColorAboutBack" />
            <gradient android:angle="-90" android:startColor="@color/btnMainPressedStartColorAboutBack" android:endColor="@color/btnMainPressedEndColorAboutBack"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="22dip" />
            <stroke android:width="1dip" android:color="@color/btnMainPressedBorderColorAboutBack" />
            <solid android:color="@color/btnMainShapeEndColorAboutBack"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="22dip" />
            <stroke android:width="1dip" android:color="@color/btnMainShapeBorderColorAboutBack" />
            <gradient android:angle="-90" android:startColor="@color/btnMainShapeStartColorAboutBack" android:endColor="@color/btnMainShapeEndColorAboutBack" />
        </shape>
    </item>
</selector>