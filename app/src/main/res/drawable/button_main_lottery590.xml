<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="22dip" />
            <stroke android:width="1dip" android:color="@color/btnMainPressedBorderColor590" />
            <gradient android:angle="-90" android:startColor="@color/btnMainPressedStartColor590" android:endColor="@color/btnMainPressedEndColor590"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="22dip" />
            <stroke android:width="1dip" android:color="@color/btnMainPressedBorderColor590" />
            <solid android:color="@color/btnMainShapeEndColor590"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="22dip" />
            <stroke android:width="1dip" android:color="@color/btnMainShapeBorderColor590" />
            <gradient android:angle="-90" android:startColor="@color/btnMainShapeStartColor590" android:endColor="@color/btnMainShapeEndColor590" />
        </shape>
    </item>
</selector>